{
  "name": "ant-design-pro",
  "version": "1.2.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 GENERATE_SOURCEMAP=false umi build",
    "build": "GENERATE_SOURCEMAP=false umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_UI=none GENERATE_SOURCEMAP=false umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none GENERATE_SOURCEMAP=false umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "cypress-test": "cd tests && cd ui_automation_hrms && cypress open",
    "cypress-jenkin": "cd tests && cd ui_automation_hrms && node cypress_config_run.js"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.2",
    "@ant-design/icons": "4.0.0",
    "@ant-design/pro-descriptions": "1.0.4",
    "@ant-design/pro-layout": "6.4.15",
    "@ant-design/pro-table": "2.5.13",
    "@ctrl/tinycolor": "^3.3.1",
    "@dabeng/react-orgchart": "^1.0.0",
    "@dhaiwat10/react-link-preview": "^1.14.1",
    "@tinymce/tinymce-react": "^3.9.0",
    "antd": "4.17.1",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "browser-image-compression": "^2.0.0",
    "city-timezones": "^1.2.1",
    "classnames": "^2.2.6",
    "csvtojson": "^2.0.10",
    "cypress": "^8.3.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "file-saver": "^2.0.5",
    "html-react-parser": "^1.4.5",
    "html2canvas": "^1.0.0-rc.7",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "path-to-regexp": "2.4.0",
    "prettier": "^2.1.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "quill-mention": "^3.0.0",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^13.1.0",
    "react-collapse": "^5.1.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.8.6",
    "react-dropzone": "^11.2.1",
    "react-google-charts": "^3.0.15",
    "react-image-crop": "^8.6.5",
    "react-joyride": "^2.4.0",
    "react-lazyload": "^3.2.0",
    "react-quill": "^1.3.5",
    "react-scroll": "^1.8.2",
    "react-select": "^5.2.2",
    "react-signature-canvas": "^1.0.3",
    "react-sortable-hoc": "^2.0.0",
    "react-to-print": "^2.12.3",
    "recharts": "^2.1.6",
    "recharts-to-png": "2.0.0",
    "socket.io-client": "^4.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "umi": "^3.2.22",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.18",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.2.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^2.1.3",
    "@umijs/preset-ui": "^2.0.9",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.0",
    "puppeteer-core": "^5.0.0",
    "stylelint": "^14.9.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
