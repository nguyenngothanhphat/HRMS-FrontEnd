    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.0",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "moment-range": "^4.0.1",
    "npm": "^6.10.3",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^3.0.0",
    "payment": "^2.3.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
