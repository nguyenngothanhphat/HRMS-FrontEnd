    "react-excel-renderer": "^1.1.0",
    "react-fittext": "^1.0.0",
    "react-google-maps": "^9.4.5",
    "react-google-recaptcha": "^2.0.1",
    "react-infinite-scroll-component": "^4.5.3",
    "react-media": "^1.9.2",
    "react-window-size": "^1.2.2",
    "react-zoom-pan-pinch": "^1.4.5",
    "recompose": "^0.30.0",
    "umi": "^2.7.7",
